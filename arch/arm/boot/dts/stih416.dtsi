			strap-8bit;
			reg		=	<0xfe340000 0x100>,
						<0xfe343c00 0x100>,
						<0xfe343e00 0x100>,
						<0xfe343f00 0x100>;
			reg-names = "wrapper", "ohci", "ehci", "protocol";

			interrupts	=  <0 154 0>, <0 155 0>;
			interrupt-names = "ehci","ohci";
			device-config = <&usb3_dev_config>;
			usb3_dev_config: usb_dev_config-3 {
				#stm-sysconf-cells = <4>;
				padcfg-0 	= <&padcfg_usb3>;
				device-seqs = <&usb_dev_seqs>;
				sysconfs{
					USB_HOST_PWR = <&sysconf 2525 6 6>;
					USB_PWR_ACK = <&sysconf 2583 5 5>;
					USB_IN_DC_SHIFT = <&sysconf 2520 8 8>;
					USB_IN_EDGE_CTRL = <&sysconf 2520 9 9>;
				};
			};

		};

	/* SAS PWM MODULE */
