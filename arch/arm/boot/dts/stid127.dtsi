	soc {
		#address-cells = <1>;
		#size-cells = <1>;
		#interrupt-cells = <3>;
		interrupt-parent = <&intc>;
		ranges;
		compatible	= "simple-bus";
		id		= <&sysconf 772 0 31>;
		reset		= <&sysconf 773 0 0>;

		/* FSM */
		spifsm:	spifsm@fe302000{
			compatible	= "st,spi-fsm";
			status		= "disabled";
			reg =	<0xfe302000 0x1000>;
			reg-names = "spi-fsm";
			padcfg-0 	= <&padcfg_fsm>;
			caps-handle = <&fsm_caps>;
			fsm_caps: fsm-caps {
				no-read-repeat;
				no-write-repeat;
				read-status-clkdiv4;
				dual-mode;		/* DUAL mode */
				quad-mode;		/* QUAD mode */
			};
		};

		pwm0: pwm@fe510000{
			compatible	= "st,pwm";
			reg		= <0xfe510000 0x68>;
			interrupts	=  <0 17 0>;
			status = "disabled";
			padcfg-names = "chan0", "chan1";
			padcfg-0 = <&padcfg_pwm0_chan0>;
			padcfg-1 = <&padcfg_pwm0_chan1>;
		};

		/* Comms block ASCs in SASG2 */
		uart0: uart@fe530000{
			compatible	= "st,asc";
			status = "disabled";
			reg		= <0xfe530000 0x2c>;
			interrupts	=  <0 25 0>;
			padcfg-0 	= <&padcfg_uart0>;

		};

		uart1: uart@fe531000 {
			compatible	= "st,asc";
			status = "disabled";
			reg		= <0xfe531000 0x2c>;
			interrupts	=  <0 26 0>;
			padcfg-0 	= <&padcfg_uart1>;
		};

		uart2: uart@fe532000{
			compatible	= "st,asc";
			status = "disabled";
			reg		= <0xfe532000 0x2c>;
			interrupts	=  <0 27 0>;
			padcfg-0 	= <&padcfg_uart2>;
		};

		/* telss */
		uart3: uart@feba6000 {
			compatible	= "st,asc";
			status = "disabled";
			reg		= <0xfeba6000 0x2c>;
			interrupts	=  <0 144 0>;
			padcfg-0 	= <&padcfg_uart3>;
		};

		ethernet0:ethernet@feb00000 {
			compatible	= "snps,dwmac";
			status = "disabled";
			device_type = "network";

			reg = <0xfeb00000 0x8000>;
			interrupts = <0 31 0>, <0 30 0>, <0 29 0>;
			interrupt-names = "macirq", "eth_wake_irq", "eth_lpi";

			snps,enh-desc;
			snps,pbl = <32>;
			snps,mixed-burst;
			snps,bugged_tx_coe;

			amba-config	= <&ethernet_amba_bridge_config>;
			st,phy-clk-name = "CLK_ETH0";
			st,fix-mac-speed = <&gmac_speed_fixups>;
		};

		stmfp:ethernet@fee80000 {
			compatible	= "st,fplite";
			status = "disabled";
			device_type = "network";

			reg = <0xfee80000 0x40000>;
			interrupts = <0 101 0>, <0 102 0>;
			interrupt-names = "fptxdmairq", "fprxdmairq";

			st,fp_clk_rate = <200>;
		};

		ethernet_amba_bridge_config: stm-eth-amba-bridge {
			reg-offset = <0x1ffc>;
			bridge_type2;
			chunks_in_msg =	<1>;
			packets_in_chunk = <2>;
			write_posting;
			max_opcode_LD64_ST64;
			threshold = <512>;
			sd_config_missing;
			read_ahead;
		};

		/*
			ISVE - Virtual Interfaces

			Downstream queue:
				docsis_base + 0x2E400 + (queue * 0x20)
			Upstream queue:
				docsis_base + 0x1A400 + (queue * 0x28)
		*/
		if17:isve_queue_3 {
			compatible	= "st,isve";
			status = "disabled";
			device_type = "network";
			reg = <0xfee2e460 0x20>, <0xfee1a478 0x20>;
			interrupts = <0 78 0>, <0 89 0>;
			interrupt-names = "isveirq_ds", "isveirq_us";

			isve,downstream_queue_size = <32>;
			isve,upstream_queue_size = <32>;
			isve,queue_number = <3>;
			isve,skip_hdr = <0x2>; /* two extra bytes have to be removed */
			isve,hw_rem_hdr = <0x3>; /* remove docsis and extra HDR */
			isve,ifname = "if17";
		};

		if18:isve_queue_4 {
			compatible	= "st,isve";
			status = "disabled";
			device_type = "network";
			reg = <0xfee2e480 0x20>, <0xfee1a4a0 0x20>;
			interrupts = <0 79 0>, <0 90 0>;
			interrupt-names = "isveirq_ds", "isveirq_us";

			isve,downstream_queue_size = <32>;
			isve,upstream_queue_size = <32>;
			isve,queue_number = <4>;
			isve,skip_hdr = <0x2>; /* two extra bytes have to be removed */
			isve,hw_rem_hdr = <0x3>; /* remove docsis and extra HDR */
			isve,ifname = "if18";
		};

		if16:isve_queue_5 {
			compatible	= "st,isve";
			status = "disabled";
			device_type = "network";
			reg = <0xfee2e4a0 0x20>, <0xfee1a4c8 0x20>;
			interrupts = <0 80 0>, <0 91 0>;
			interrupt-names = "isveirq_ds", "isveirq_us";

			isve,downstream_queue_size = <32>;
			isve,upstream_queue_size = <32>;
			isve,queue_number = <5>;
			isve,skip_hdr = <0x2>; /* two extra bytes have to be removed */
			isve,hw_rem_hdr = <0x3>; /* remove docsis and extra HDR */
			isve,ifname = "if16";
		};

		if1:isve_queue_7 {
			compatible	= "st,isve";
			status = "disabled";
			device_type = "network";
			reg = <0xfee2e4e0 0x20>, <0xfee1a518 0x20>;
			interrupts = <0 82 0>, <0 93 0>;
			interrupt-names = "isveirq_ds", "isveirq_us";

			isve,downstream_queue_size = <32>;
			isve,upstream_queue_size = <32>;
			isve,queue_number = <7>;
			isve,skip_hdr = <0x2>; /* two extra bytes have to be removed */
			isve,hw_rem_hdr = <0x3>; /* remove docsis and extra HDR */
			isve,ifname = "if1";
		};

		/* temp */
		temp_pwr_seqs: temp-pwr-seqs{
			power-on-seq{
				step0 {
					TEMP_PWR = <1>;
					type = "sysconf";
				};
			};
			power-off-seq{
				step0 {
					TEMP_PWR = <0>;
					type = "sysconf";
				};
			};
		};

		temp0_dev_config: temp0-dev-config {
			device-seqs = <&temp_pwr_seqs>;
			sysconfs {
				TEMP_PWR = <&sysconf 743 7 7>;
				DCORRECT = <&sysconf 743 2 6>;
				OVERFLOW = <&sysconf 767 9 9>;
				DATA = <&sysconf 767 11 18>;
			};
		};

		/* Internal Thermal sensor */
		temp0{
			compatible    = "st,temp";
			status = "okay";
			device-config = <&temp0_dev_config>;
		};

		/* FDMA */
		fdma_fw_regs: fdma-fw-regs{
			fw-rev-id    = <0x10000>;
			fw-mchi-rx-nb-cur = <0x100c4>;
			fw-mchi-rx-nb-all = <0x100c8>;
			fw-cmd-statn = <0x10200>;
			fw-req-ctln  = <0x10240>;
			fw-ptrn      = <0x10800>;
			fw-ctrln     = <0x10804>;
			fw-cntn      = <0x10808>;
			fw-saddrn    = <0x1080c>;
			fw-daddrn    = <0x10810>;
			fw-node-size = <128>;
		};

		fdma_hw_config: fdma-hw-config {
			slim-reg-id       = <0x0>;
			slim-reg-ver      = <0x4>;
			slim-reg-en       = <0x8>;
			slim-reg-clk-gate = <0xc>;
			slim-reg-slim-pc  = <0x20>;

			dmem-offset = <0x10000>;
			dmem-size   = <0x3000>;

			periph-reg-sync-reg = <0x17f88>;
			periph-reg-cmd-sta  = <0x17fc0>;
			periph-reg-cmd-set  = <0x17fc4>;
			periph-reg-cmd-clr  = <0x17fc8>;
			periph-reg-cmd-mask = <0x17fcc>;
			periph-reg-int-sta  = <0x17fd0>;
			periph-reg-int-set  = <0x17fd4>;
			periph-reg-int-clr  = <0x17fd8>;
			periph-reg-int-mask = <0x17fdc>;

			imem-offset = <0x18000>;
			imem-size   = <0x8000>;
		};

