		sbc_uart0: uart@fe530000 {
			compatible	= "st,asc", "simple-bus";
			status = "disabled";
			reg		= <0xfe530000 0x2c>;
			interrupts	=  <0 209 0>;
			padcfg-0 	= <&padcfg_uart5>;

			st,clk-id	= "sbc_comms_clk";
		};

		/* SBC_UART1 */
		sbc_uart1: uart@fe531000 {
			compatible	= "st,asc", "simple-bus";
			status = "disabled";
			reg		= <0xfe531000 0x2c>;
			interrupts	=  <0 210 0>;
			padcfg-0 	= <&padcfg_uart6>;
