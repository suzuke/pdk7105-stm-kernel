		lirc: lirc@fe518000 {
			compatible	= "st,lirc", "simple-bus";
			status = "disabled";
			reg		= <0xfe518000 0x234>;
			interrupts	=  <0 203 0>;
			sys-clk	= "sbc_comms_clk";
			irb-clock		= <0>;
			irb-clkdiv		= <0>;
			irb-period-mult	= <0>;
			irb-period-div	= <0>;
			irb-ontime-mult	= <0>;
			irb-ontime-div	= <0>;
			irb-rxmax-period	= <0x5000>;
			irb-version		= <0>;
			sys-clkdiv		= <1>;
			rx-polarity		= <1>;
			sub-carrwidth	= <0>;

			device-config = <&lirc_ir_device_config>;

			lirc_ir_device_config: lirc-ir-dev-config {
				padcfg-0	= <&padcfg_ir>;
				device-seqs = <&lirc_dev_seqs>;
				sysconfs{
					irb-enabled	= <&lpm_sysconf 1 6 6>;
					irb-normal-mode = <&lpm_sysconf 1 7 7>;
				};
			};

			lirc_uhf_device_config: lirc-uhf-dev-config {
				padcfg-0	= <&padcfg_uhf>;
				device-seqs = <&lirc_dev_seqs>;
				sysconfs{
					irb-enabled	= <&lpm_sysconf 1 6 6>;
					irb-normal-mode = <&lpm_sysconf 1 7 7>;
				};
			};

			lirc_tx_device_config: lirc-tx-dev-config {
				padcfg-0	= <&padcfg_ir &padcfg_tx>;
				device-seqs = <&lirc_dev_seqs>;
				sysconfs{
					irb-enabled	= <&lpm_sysconf 1 6 6>;
					irb-normal-mode = <&lpm_sysconf 1 7 7>;
				};
			};
			lirc_tx_od_device_config: lirc-tx-od-dev-config {
				padcfg-0	= <&padcfg_ir &padcfg_tx_od>;
				device-seqs = <&lirc_dev_seqs>;
				sysconfs{
					irb-enabled	= <&lpm_sysconf 1 6 6>;
					irb-normal-mode = <&lpm_sysconf 1 7 7>;
				};
			};

			lirc_dev_seqs: lirc-dev-seqs{
				init-seq{
					step0 {
						irb-enabled = <1>;
						type = "sysconf";
					};
					step1 {
						irb-normal-mode = <0>;
						type = "sysconf";
					};
				};
			};
		};

		i2c0: i2c@fed40000{
			compatible	= "st,i2c", "simple-bus";
			status = "disabled";
			reg		= <0xfed40000 0x110>;
			interrupts	=  <0 187 0>;
			padcfg-0 	= <&padcfg_i2c0_sda_pio9_3 &padcfg_i2c0_sclk_pio9_2>;

		};

		i2c1: i2c@fed41000{
