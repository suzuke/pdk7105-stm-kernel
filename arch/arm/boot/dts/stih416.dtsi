		/* Comms block ASCs in SASG2 */
		uart0: uart@fed30000{
			compatible	= "st,asc", "simple-bus";
			status = "disabled";
			reg		= <0xfed30000 0x2c>;
			interrupts	=  <0 195 0>;
			padcfg-0 	= <&padcfg_uart0>;

		};

		uart1: uart@fed31000 {
			compatible	= "st,asc", "simple-bus";
			status = "disabled";
			reg		= <0xfed31000 0x2c>;
			interrupts	=  <0 196 0>;
			padcfg-0 	= <&padcfg_uart1>;
