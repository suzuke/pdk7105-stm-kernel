		pwm1: pwm-1{
			compatible	= "st,pwm", "simple-bus";
			reg		= <0xfe510000 0x68>;
			interrupts	=  <0 202 0>;
			status = "disabled";
			padcfg-names = "chan0", "chan1", "chan2", "chan3";
			padcfg-0 = <&padcfg_pwm1_chan0>;
