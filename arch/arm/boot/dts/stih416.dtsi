		sdhci0:sdhci@fe81e000 {
			compatible	= "st,sdhci", "simple-bus";
			status = "disabled";
			reg	= <0xfe81e000  0x1000>;
			interrupts	=  <0 127 0>;
			interrupt-names = "mmcirq";
			padcfg-0 	= <&padcfg_mmc0>;
		};

		sdhci1:sdhci@fe81f000 {
			compatible	= "st,sdhci", "simple-bus";
			status = "disabled";
			reg	= <0xfe81f000  0x1000>;
			interrupts	=  <0 128 0>;
			interrupt-names = "mmcirq";
			padcfg-0 	= <&padcfg_mmc1>;
		};
		ethernet0:ethernet@fe810000 {
			compatible	= "snps,dwmac", "simple-bus";
			status = "disabled";
			device_type = "network";

			reg = <0xfe810000 0x8000>;
			interrupts = <0 133 0>, <0 134 0>, <0 135 0>;
			interrupt-names = "macirq", "eth_wake_irq", "eth_lpi";

			snps,enh-desc;
			snps,bugged-jumbo;
			snps,force-sf-dma-mode;
			snps,pbl = <32>;
			snps,mixed-burst;

			amba-config	= <&ethernet_amba_bridge_config>;
			st,phy-clk-name = "CLK_S_GMAC0_PHY";
			st,fix-mac-speed = <&gmac_speed_fixups>;
		};

		ethernet1:ethernet@fef08000 {
			device_type = "network";
			compatible	= "snps,dwmac", "simple-bus";
			status = "disabled";

			reg = <0xfef08000 0x8000>;
			interrupts = <0 136 0>, <0 137 0>, <0 138 0>;
			interrupt-names = "macirq", "eth_wake_irq", "eth_lpi";

			snps,enh-desc;
			snps,bugged-jumbo;
			snps,force-sf-dma-mode;
			snps,pbl = <32>;
			snps,mixed-burst;

			amba-config	= <&ethernet_amba_bridge_config>;
			st,phy-clk-name = "CLK_S_ETH1_PHY";
			st,fix-mac-speed = <&gmac_speed_fixups>;

		};

		ethernet_amba_bridge_config: stm-eth-amba-bridge {
			reg-offset		= <0x1ffc>;
			bridge_type2;
			chunks_in_msg	=	<1>;
			packets_in_chunk	=	<2>;
			write_posting;
			max_opcode_LD64_ST64;
			threshold		=	<512>;
			sd_config_missing;
			read_ahead;
		};

	/* temp */
		temp_pwr_seqs: temp-pwr-seqs{
			init-seq{
				step0 {
					clk-name = "CLK_S_THSENS";
					clk-rate = <156250>;
					clk-parent = "CLK_S_C_FS0_CH2";
					clk-parent-rate = <625000>;
					type = "clock";
				};
			};
			power-on-seq{
				step0 {
					TEMP_PWR = <1>;
					type = "sysconf";
				};
			};
			power-off-seq{
				step0 {
					TEMP_PWR = <0>;
					type = "sysconf";
				};
			};
		};

		temp0_dev_config: temp0-dev-config {
			device-seqs = <&temp_pwr_seqs>;
			sysconfs {
				TEMP_PWR = <&sysconf 1552 9 9>;
				DCORRECT = <&sysconf 1552 4 8>;
				OVERFLOW = <&sysconf 1554 8 8>;
				DATA = <&sysconf 1594 10 16>;
			};
		};

		/* Thermal sensor on SAS */
		temp0{
			compatible    = "st,temp", "simple-bus";
		        status = "okay";
			device-config = <&temp0_dev_config>;
		};

		/* FDMA */
		fdma_fw_regs: fdma-fw-regs{
			fw-rev-id    = <0x10000>;
			fw-cmd-statn = <0x10200>;
			fw-req-ctln  = <0x10240>;
			fw-ptrn      = <0x10800>;
			fw-cntn      = <0x10808>;
			fw-saddrn    = <0x1080c>;
			fw-daddrn    = <0x10810>;
			fw-node-size = <128>;
		};

		fdma_hw_config: fdma-hw-config {
			slim-reg-id       = <0x0>;
			slim-reg-ver      = <0x4>;
			slim-reg-en       = <0x8>;
			slim-reg-clk-gate = <0xc>;

			dmem-offset = <0x10000>;
			dmem-size   = <0x3000>;

			periph-reg-sync-reg = <0x17f88>;
			periph-reg-cmd-sta  = <0x17fc0>;
			periph-reg-cmd-set  = <0x17fc4>;
			periph-reg-cmd-clr  = <0x17fc8>;
			periph-reg-cmd-mask = <0x17fcc>;
			periph-reg-int-sta  = <0x17fd0>;
			periph-reg-int-set  = <0x17fd4>;
			periph-reg-int-clr  = <0x17fd8>;
			periph-reg-int-mask = <0x17fdc>;

			imem-offset = <0x18000>;
			imem-size   = <0x8000>;
		};

		/* FDMA_0_MPE: */
		fdma0_mpe:fdma-mpe@0{
			compatible	= "st,fdma", "simple-bus";
			reg		= <0xfd600000 0x20000>;
			interrupts	=  <0 10 0>;
			hw-conf = <&fdma_hw_config>;
			fw-regs = <&fdma_fw_regs>;
			xbar	= <0>;

		};

		/* FDMA_1_MPE: */
		fdma1_mpe:fdma-mpe@1{
			compatible	= "st,fdma", "simple-bus";
			reg		= <0xfd620000 0x20000>;
			interrupts	=  <0 12 0>;
			hw-conf = <&fdma_hw_config>;
			fw-regs = <&fdma_fw_regs>;
			xbar	= <0>;
		};

		/* FDMA_2_MPE: */
		fdma2_mpe:fdma-mpe@2{
			compatible	= "st,fdma", "simple-bus";
			reg		= <0xfd640000 0x20000>;
			interrupts	=  <0 14 0>;
			hw-conf = <&fdma_hw_config>;
			fw-regs = <&fdma_fw_regs>;
			xbar	= <0>;
		};

		fdma_xbar_mpe: fdma-xbar-mpe {
			compatible	= "st,fdma-xbar", "simple-bus";
			reg		= <0xfd6df000 0x1000>;
			first-fdma	= <0>;
			last-fdma	= <2>;
		};

		fdma_xbar_sas: fdma-xbar-sas {
			compatible	= "st,fdma-xbar", "simple-bus";
			reg		= <0xfee61000 0x1000>;
			first-fdma	= <3>;
			last-fdma	= <4>;
		};

		/* FDMA_100: SAS FDMA 0 */
		fdma0_sas:fdma-sas@0{
			compatible	= "st,fdma", "simple-bus";
			reg		= <0xfea00000 0x20000>;
			interrupts	=  <0 121 0>;
			hw-conf = <&fdma_hw_config>;
			fw-regs = <&fdma_fw_regs>;
			xbar	= <1>;
		};

		/* FDMA_101: SAS FDMA 1 */
		fdma1_sas:fdma-sas@1{
			compatible	= "st,fdma", "simple-bus";
			reg		= <0xfea20000 0x20000>;
			interrupts	=  <0 124 0>;
			hw-conf = <&fdma_hw_config>;
			fw-regs = <&fdma_fw_regs>;
			xbar	= <1>;
		};
	};
};
