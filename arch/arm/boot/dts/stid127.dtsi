	sysconf: sysconf{
		#sysconf-group-cells	= <3>;
		sysconf-groups = <43  61  0>,
				 <254 289 1>,
				 <600 604 2>,
				 <700 779 3>,
				 <900 946 4>,
				 <1000 1079 5>,
				 <1200 1270 6>,
				 <1400 1479 7>;

		sysconf-names  = "SYSCONF_WEST",
				 "SYSCONF_SOUTH",
				 "SYSCONF_DOCSIS",
				 "SYSCONF_CPU",
				 "SYSCONF_HD",
				 "SYSCONF_PWEST",
				 "SYSCONF_PSOUTH",
				 "SYSCONF_PEAST";
	};

	sysconfs {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
		compatible	= "simple-bus";
		sysconf@febf0000{
			compatible	= "st,sysconf";
			reg	=	<0xfebf0000 0x1000>;
			group@0{
				group	= <0>;
				offset	= <0xac>;
				start	= <43>;
				group-name	=  "SYSCONF_WEST";
			};
		};
		sysconf@fefa0000{
			compatible	= "st,sysconf";
			reg	=	<0xfefa0000 0x1000>;
			group@0{
				group	= <1>;
				offset	= <0xd8>;
				start	= <254>;
				group-name	=  "SYSCONF_SOUTH";
			};
		};
		sysconf@fef90000{
			compatible	= "st,sysconf";
			reg	=	<0xfef90000 0x1000>;
			group@0 {
				group	= <2>;
				offset	= <0>;
				start	= <600>;
				group-name	=  "SYSCONF_DOCSIS";
			};
		};

		sysconf@fe9a0000{
			compatible	= "st,sysconf";
			reg	=	<0xfe9a0000 0x1000>;
			group@0 {
				group	= <3>;
				offset	= <0>;
				start	= <700>;
				group-name	=  "SYSCONF_CPU";
			};
		};
		sysconf@fe930000{
			compatible	= "st,sysconf";
