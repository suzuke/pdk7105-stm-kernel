	gpio-irqmux {
		#interrupt-cells = <3>;
                #address-cells = <1>;
                #size-cells = <1>;
		interrupt-parent = <&intc>;
		ranges;
		/* PIO0-4: SBC_PIO (aka pio_sas_sbc, PIO_SBC) */
		/* PIO40 */
		gpio-irqmux@fe61f080{
			compatible	= "st,gpio-irqmux";
			reg =	<0xfe61f080 0x4>;
			interrupts = <0 182 0x0>;
			first-port = <0>;
			ports = <6>;
		};

		/* PIO5-12: PIO_FRONT (aka pio_sas_front) */
		/* PIO30-31 */
		gpio-irqmux@fee0f080{
			compatible	= "st,gpio-irqmux";
			reg =	<0xfee0f080 0x4>;
			interrupts = <0 183 0x0>;
			first-port = <6>;
			ports = <10>;
		};

		/* PIO13-18: PIO_REAR (aka pio_sas_rear) */
		gpio-irqmux@fe82f080{
			compatible	= "st,gpio-irqmux";
			reg =	<0xfe82f080 0x4>;
			interrupts = <0 184 0x0>;
			first-port = <16>;
			ports = <6>;
		};

		/* PIO100-102: PIO_RIGHT (aka MPE_PIO, PIO0_MPE) */
		gpio-irqmux@fd6bf080{
			compatible	= "st,gpio-irqmux";
			reg =	<0xfd6bf080 0x4>;
			interrupts = <0 113 0x0>;
			first-port = <22>;
			ports = <3>;
		};

		/* PIO103-107: PIO_LEFT (aka PIO1_MPE) */
		gpio-irqmux@fd33f080{
			compatible	= "st,gpio-irqmux";
			reg =	<0xfd33f080 0x4>;
			interrupts = <0 114 0x0>;
			first-port = <25>;
			ports = <5>;
		};
	};

	soc {
	        #address-cells = <1>;
	        #size-cells = <1>;
		#interrupt-cells = <3>;
		interrupt-parent = <&intc>;
		ranges;
		compatible	= "simple-bus";
		id		= <&sysconf 9516 0 31>;
		reset		= <&sysconf 504 0 0>;
		emis{
		        #address-cells = <1>;
		        #size-cells = <1>;
			#interrupt-cells = <3>;
			interrupt-parent = <&intc>;
			ranges;
			emi {
				compatible	= "st,emi";
				reg		=	<0 0x10000000>,
							<0xfe900000 0x874>,
							<0xfef01000 0x80>;
				reg-names = "emi memory", "emi4 config", "emiss config";
				device-config = <&emi_dev_config>;
				emi_dev_config: emi-dev-config {
					#stm-sysconf-cells = <4>;
					device-seqs = <&emi_dev_seqs>;
					sysconfs{
						EMI_PWR = <&sysconf 1500 0 0>;
						EMI_ACK = <&sysconf 1578 0 0>;
					};
				};

				emi_dev_seqs: emi-dev-seqs{
					power-on-seq{
						step0 {
							EMI_PWR = <0>;
							type = "sysconf";
						};
						step1{
							EMI_ACK = <0>;
							type = "sysconf-feedback";
						};
					};
					power-off-seq{
						step0 {
							EMI_PWR = <1>;
							type = "sysconf";
						};
						step1{
							EMI_ACK = <1>;
							type = "sysconf-feedback";
						};
					};
				};
			};
		};

		nandafm: nand-afm{
			compatible	= "st,nand-afm", "simple-bus";
			reg =	<0xfe901000 0x1000>, <0xfef00800 0x0800>;
			reg-names = "nand_mem", "nand_dma";
			interrupts = <0 139 0x0>;
			interrupt-names = "nand_irq";
			status = "disabled";
		};

		nandbch: nand-bch {
			compatible	= "st,nand-bch", "simple-bus";
			reg =	<0xfe901000 0x1000>, <0xfef00800 0x0800>;
			reg-names = "nand_mem", "nand_dma";
			interrupts = <0 139 0x0>;
			interrupt-names = "nand_irq";
			status = "disabled";
			st,bch-ecc-mode	= "auto";
		};

		/* FSM */
		spifsm:	spifsm@fe902000{
			compatible	= "st,spi-fsm", "simple-bus";
			status		= "disabled";
			reg =	<0xfe902000 0x1000>;
			reg-names = "spi-fsm";
			padcfg-0 	= <&padcfg_fsm>;
			caps-handle = <&fsm_caps>;
			fsm_caps: fsm-caps {
				no-read-repeat;
				no-write-repeat;
				read-status-clkdiv4;
				dual-mode;		/* DUAL mode */
				quad-mode;		/* QUAD mode */
			};
		};

		usb0_dev_seqs: usb0-dev-seqs{
			init-seq{

				step0 {
					USB_IS_OSC_STABLE = <1>;
					type = "sysconf";
				};
				step1 {
					USB_IN_DC_SHIFT = <0>;
					type = "sysconf";
				};
				step2 {
					USB_IN_EDGE_CTRL = <1>;
					type = "sysconf";
				};
			};
			power-on-seq{
				step0 {
					USB_HOST_PWR = <0>;
					type = "sysconf";
				};
				step1{
					USB_PWR_ACK = <0>;
					type = "sysconf-feedback";
				};
			};
			power-off-seq{
				step0 {
					USB_HOST_PWR = <1>;
					type = "sysconf";
				};
				step1{
					USB_PWR_ACK = <1>;
					type = "sysconf-feedback";
				};
			};
		};

		usb_dev_seqs: usb-dev-seqs{
			init-seq{
				step0 {
					USB_IN_DC_SHIFT = <0>;
					type = "sysconf";
				};
				step1 {
					USB_IN_EDGE_CTRL = <1>;
					type = "sysconf";
				};
			};
			power-on-seq{
				step0 {
