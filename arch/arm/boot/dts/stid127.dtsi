				group	= <4>;
				offset	= <0>;
				start	= <900>;
				group-name	=  "SYSCONF_HD";
			};
		};

		sysconf@fec00000{
			compatible	= "st,sysconf";
			reg	=	<0xfec00000 0x1000>;
