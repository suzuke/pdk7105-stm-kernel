	/* start end group  */
	lpm_sysconf: lpmsysconf{
		#sysconf-group-cells	= <3>;
		sysconf-groups = <0 10 8>;
		sysconf-names  = "LPM_CFG_REGS";
	};

	sysconf: sysconf{
		#sysconf-group-cells	= <3>;
		sysconf-groups = <0     999 0>,
				 <1000 1999 1>,
				 <2000 2999 2>,
				 <5000 5999 3>,
				 <6000 6999 4>,
				 <7000 7999 5>,
				 <8000 8999 6>,
				 <9000 9999 7>;

		sysconf-names  = "SYSCFG_SBC",
				 "SYSCFG_FRONT",
				 "SYSCFG_REAR",
				 "SYSCFG_FVDP_FE",
				 "SYSCFG_FVDP_LITE",
				 "SYSCFG_CPU",
				 "SYSCFG_COMPO",
				 "SYSCFG_TRANSPORT";
	};

	sysconfs {
                #address-cells = <1>;
                #size-cells = <1>;
		ranges;
		compatible	= "simple-bus";
		sysconf@fe600000{
			compatible	= "st,sysconf", "simple-bus";
			reg	=	<0xfe600000 0x1000>;
			group@0{
				group	= <0>;
				offset	= <0>;
				start	= <0>;
				group-name	=  "SYSCFG_SBC";
			};
		};
		sysconf@fee10000{
			compatible	= "st,sysconf", "simple-bus";
			reg	=	<0xfee10000 0x1000>;
			group@0{
				group	= <1>;
				offset	= <0>;
				start	= <1000>;
				group-name	=  "SYSCFG_FRONT";
			};
		};
		sysconf@fe830000{
			compatible	= "st,sysconf", "simple-bus";
			reg	=	<0xfe830000 0x1000>;
			group@0 {
				group	= <2>;
				offset	= <0>;
				start	= <2000>;
				group-name	=  "SYSCFG_REAR";
			};
		};

		/* MPE */
		sysconf@fddf0000{
			compatible	= "st,sysconf", "simple-bus";
			reg	=	<0xfddf0000 0x1000>;
			group@0 {
				group	= <3>;
				offset	= <0>;
				start	= <5000>;
				group-name	=  "SYSCFG_FVDP_FE";
			};
		};
		sysconf@fd6a0000{
			compatible	= "st,sysconf", "simple-bus";
			reg	=	<0xfd6a0000 0x1000>;
			group@0 {
				group	= <4>;
				offset	= <0>;
				start	= <6000>;
				group-name	=  "SYSCFG_FVDP_LITE";
			};
		};

		sysconf@fdde0000{
			compatible	= "st,sysconf", "simple-bus";
			reg	=	<0xfdde0000 0x1000>;
			group@0 {
				group	= <5>;
				offset	= <0>;
				start	= <7000>;
				group-name	=  "SYSCFG_CPU";
			};
		};

		sysconf@fd320000{
			compatible	= "st,sysconf", "simple-bus";
			reg	=	<0xfd320000 0x1000>;
			group@0 {
				group	= <6>;
				offset	= <0>;
				start	= <8000>;
				group-name	=  "SYSCFG_COMPO";
			};
		};

		sysconf@fd690000{
			compatible	= "st,sysconf", "simple-bus";
			reg	=	<0xfd690000 0x1000>;
			group@0{
				group	= <7>;
				offset	= <0>;
				start	= <9000>;
				group-name	=  "SYSCFG_TRANSPORT";
			};
		};

		sysconf@fe4b5100{
			compatible	= "st,sysconf", "simple-bus";
			reg	=	<0xfe4b5100 0x54>;
			group@0 {
				group	= <8>;
				offset	= <0>;
				start	= <0>;
				group-name	=  "LPM_CFG_REGS";
			};
		};
	};


