			status = "disabled";
			padcfg-names = "chan0", "chan1", "chan2", "chan3";
			padcfg-0 = <&padcfg_pwm0_chan0>;
			padcfg-1 = <&padcfg_pwm0_chan1>;
			padcfg-2 = <&padcfg_pwm0_chan2>;
			padcfg-3 = <&padcfg_pwm0_chan3>;
		};

