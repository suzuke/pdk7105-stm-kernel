		i2c7: i2c@fed47000{
			compatible	= "st,i2c", "simple-bus";
			status = "disabled";
			reg		= <0xfed47000 0x110>;
			interrupts	=  <0 194 0>;
			padcfg-0 	= <&padcfg_i2c7>;
		};

		/* SBC SSC0 */
		sbc_i2c0: i2c@fe540000{
			compatible	= "st,i2c", "simple-bus";
			status = "disabled";
			reg		= <0xfe540000 0x110>;
			interrupts	=  <0 206 0>;
			padcfg-0 	= <&padcfg_sbc_i2c0>;
